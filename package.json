{
  "name": "kuberanix-backend-base",
  "version": "1.0.6",
  "bin": {
    "kuberanix-backend-base": "bin/install.mjs"
  },
  "description": "Boilerplate for kuberanix backend",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=production  node server.js",
    "dev": "NODE_ENV=dev  nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "lint-staged": {
    "**/*.{js,jsx,json}": [
      "eslint . --fix",
      "prettier --write ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm publish"
    }
  },
  "imports": {
    "#*": "./*",
    "#routes/*": "./routes/*",
    "#userroutes": "./routes/users/routes.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.0.1",
    "ora": "^7.0.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "eslint": "^8.50.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3"
  }
}
